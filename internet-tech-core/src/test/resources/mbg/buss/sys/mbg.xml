<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
	  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
	  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
	<context id="MySQLTables" targetRuntime="MyBatis3">

		<property name="suppressTypeWarnings" value="true" />
		<plugin type="org.mybatis.generator.plugins.RenameExampleClassPlugin">
			<property name="searchString" value="Example$" />
			<property name="replaceString" value="Criteria" />
		</plugin>
		<plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin" />
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
		<plugin type="org.mybatis.generator.plugins.CaseInsensitiveLikePlugin" />
		<plugin type="org.mybatis.generator.plugins.RenameExampleClassPlugin">
			<property name="searchString" value="Example$" />
			<property name="replaceString" value="Criteria" />
		</plugin>
		<plugin
			type="com.cw.stu.internet.tech.platform.dao.mybatis.plugin.MySqlPaginationPlugin" />
		<!-- 扩展Criteria对象，增加bean结构字段，增加一些常规搜索条件插件 -->
		<plugin
			type="com.cw.stu.internet.tech.platform.dao.mybatis.plugin.FlatCauseSupportPlugin" />
		<!--	
		<plugin
			type="com.cw.stu.internet.tech.platform.dao.mybatis.plugin.TranslateSupportPlugin" />
		-->
		<plugin
			type="com.cw.stu.internet.tech.platform.dao.mybatis.plugin.BatchOperSupportPlugin" />
		<!-- 增加字段注释说明 -->
		<plugin type="com.cw.stu.internet.tech.platform.dao.mybatis.plugin.AddCommentPlugin" />
		<!-- 分页插件 -->
		<plugin type="com.cw.stu.internet.tech.platform.dao.mybatis.plugin.PagerPlugin" />
		
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
			<property name="suppressDate" value="true" />
		</commentGenerator>

		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://192.168.10.56:3306/buss?useUnicode=true&amp;amp;characterEncoding=utf8&amp;amp;autoReconnect=true&amp;amp;profileSQL=false"
			userId="makeithappen" password="1234">
		</jdbcConnection>

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<javaModelGenerator targetPackage="com.rhtop.buss.common.dao.sys.domain"
			targetProject="./src/main/java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<sqlMapGenerator targetPackage="com.rhtop.buss.common.dao.sys.mapper"
			targetProject="./src/main/java">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<javaClientGenerator type="XMLMAPPER"
			targetPackage="com.rhtop.buss.common.dao.sys.persistence" targetProject="./src/main/java">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

		<table schema="buss" tableName="sys_biz_code" domainObjectName="BizCode"
			enableCountByExample="true" enableUpdateByExample="true"
			enableDeleteByExample="true" enableSelectByExample="true"
			selectByExampleQueryId="true">
			<property name="useActualColumnNames" value="false" />
			<generatedKey column="id" sqlStatement="MySql"
				identity="true" />
		</table>
		
		<table schema="sys" tableName="sys_biz_log" domainObjectName="BizLog"
			enableCountByExample="true" enableUpdateByExample="true"
			enableDeleteByExample="true" enableSelectByExample="true"
			selectByExampleQueryId="true">
			<property name="useActualColumnNames" value="false" />
			<generatedKey column="log_id" sqlStatement="MySql"
				identity="true" />
		</table>
		
		<table schema="sys" tableName="sys_file_attach" domainObjectName="FileAttach"
			enableCountByExample="true" enableUpdateByExample="true"
			enableDeleteByExample="true" enableSelectByExample="true"
			selectByExampleQueryId="true">
			<property name="useActualColumnNames" value="false" />
			<generatedKey column="id" sqlStatement="MySql"
				identity="true" />
		</table>
	</context>
</generatorConfiguration>